				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Audit response code\", function() {",
							"    if (pm.response.code === 200) {",
							"        pm.response.to.have.status(200);",
							"    } else {",
							"        pm.response.to.have.status(404);",
							"        console.log(\"Audit endpoint returned 404\");",
							"    }",
							"});",
							"",
							"pm.test(\"Audit response body\", function() {",
							"    var responseBody = pm.response.json();",
							"",
							"    if (pm.response.code === 200) {",
							"        pm.expect(responseBody).to.have.property('Health', 'OK');",
							"    } else {",
							"        pm.expect(responseBody).to.have.property('detail', 'Not Found');",
							"    }",
